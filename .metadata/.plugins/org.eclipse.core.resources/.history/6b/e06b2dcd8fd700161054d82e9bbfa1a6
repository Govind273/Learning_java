package package1;



public class Class1 {



	public static void main(String[] args)  {
		
	int i = 2;
	int j=3;
	int k = 10;
	
	
	try{
	   
		k= i/j;
		if(i < j ) 	throw new ArithmeticException("make i larger than j") ;
			
			
		
		
	
	}
	catch(ArithmeticException e){
	System.out.println(e);

		
	}
	
	catch(Exception e){
	
	}
	
	
	
	System.out.println(k);
	
	

}

}
