package Tree;

public class Treeimpl {
	Node root;
	
	public Treeimpl()
	{
		root=null;
	}
	
	public boolean isNull(){
		return(root == null);
	}
      

}







//Node Structure for the Tree 
class Node{
	Node left ;
    Node right;
     int data;
     
     Node(int data, Node left, Node right)
     {
    	 this.data= data;
    	 this.left = left;
    	 this.right = right;
    	 
     }
     Node(int data)
     {
    	 this.data = data;
     }
     Node(){
    	 
     }
     
     public boolean insert(int val){
    	 
    	boolean added = false;
    	
    	 if(this.data >val){
			  if(this.left == null){
				  this.left = new Node(val);
				  
				  return true;
			  }
			  else{
				added = this.left.insert(val);
			  }
			}
		 else{
			 if(this.right == null){
				 this.right = new Node(val);
				 return true;
			 }
			 else{
				 added = this.right.insert(val);
			 }
		 }
    	 
    	 return true;
     }
}