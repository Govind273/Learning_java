package Tree;

public class Treeimpl {
	Node root;
	
	public Treeimpl()
	{
		root=null;
	}
	
	public boolean isNull(){
		return(root == null);
	}

}







//Node Structure for the Tree 
class Node{
	Node left ;
    Node right;
     int data;
     
     Node(int data, Node left, Node right){
    	 this.data= data;
    	 this.left = left;
    	 this.right = right;
    	 
 }
     Node(int data){
    	 this.data = data;
     }
     Node(){
    	 
     }
}