package package1;



public class Class1 {

	public static void main(String[] args) {
		
	int i = 12;
	int j=0;
	int k = 0;
	int[] s = new int[5];
	
	try{
	 j = i/k;
	}
	catch(ArithmeticException e){
		System.out.println(e);

		for(int sa =0; sa <s.length ;sa= sa+3){
			
			s[sa]=23;
			
		}
	}
	System.out.println(j);
	

}

}
