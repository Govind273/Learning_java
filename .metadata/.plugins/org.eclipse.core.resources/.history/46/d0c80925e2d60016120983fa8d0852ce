package package1;



public class Class1 {

	public static void main(String[] args) {
		
	int i = 12;
	int j=0;
	int k = 10;
	int[] s = new int[5];
	
	try{
	 
	 for(int sa =0; sa <=s.length ;){
			
			s[sa]=23;
			sa++;
		}
	}
	catch(ArithmeticException e){
		System.out.println(e);

		
	}
	
	catch()
	System.out.println(j);
	

}

}
