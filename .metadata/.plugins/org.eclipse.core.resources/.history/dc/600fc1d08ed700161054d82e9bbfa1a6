package package1;



public class Class1 {

	private static final Exception ArithmeticException = null;

	public static void main(String[] args) throws Exception  {
		
	int i = 2;
	int j=3;
	int k = 10;
	int[] s = new int[5];
	
	try{
	   
		if(i < j ) 	throw ArithmeticException ;
			
			
		
		k= i/j;
	
	}
	catch(ArithmeticException g){
		System.out.println("I should not be less than j");

		
	}
	
	
	
	System.out.println(j);
	
	

}

}
